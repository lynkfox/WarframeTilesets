@model WFTileCounter.Models.TileImage
@{
    ViewData["Title"] = "TileImage";
}

    <div class="container">
        @Html.DisplayNameFor(x => x.TileName)
        @Html.DisplayFor(x => x.TileName)
        <div class="editor-field">
            @Html.HiddenFor(x=>x.TileName)
            @Html.ValidationMessageFor(x=>x.TileName)
        </div>
        <br>

        @Html.DisplayNameFor(x => x.ImageName)
        @Html.TextBoxFor(x => x.ImageName)

        <br>

        @Html.DisplayNameFor(x => x.ViewName)
        @Html.TextBoxFor(x => x.ViewName)

        <br>

        @Html.DisplayNameFor(x => x.AltText)
        @Html.TextBoxFor(x => x.AltText)

    </div>

